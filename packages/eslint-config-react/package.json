{
  "name": "@anvilabs/eslint-config-react",
  "version": "0.0.0-development",
  "description": "Anvilabs' ESLint config for our React/React Native apps",
  "keywords": [
    "eslint",
    "eslint config",
    "config",
    "anvilabs",
    "styleguide"
  ],
  "repository": "anvilabs/eslint-config",
  "homepage": "https://github.com/anvilabs/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/anvilabs/eslint-config/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Ayan Yenbekbay",
    "email": "ayan.yenb@gmail.com",
    "url": "https://yenbekbay.me"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "main": "index.js",
  "scripts": {
    "find-new-rules:index": "eslint-find-rules --unused test/fixtures/index.js",
    "find-new-rules:native": "eslint-find-rules --unused test/fixtures/native.js",
    "find-new-rules": "run-p --silent find-new-rules:*",
    "semantic-release": "semantic-release"
  },
  "release": {
    "extends": "semantic-release-monorepo"
  },
  "peerDependencies": {
    "@anvilabs/eslint-config": "*",
    "eslint": "*"
  },
  "dependencies": {
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-react-native": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-find-rules": "^3.2.3",
    "npm-run-all": "^4.1.3",
    "semantic-release": "15.5.2",
    "semantic-release-monorepo": "^6.0.3"
  }
}
